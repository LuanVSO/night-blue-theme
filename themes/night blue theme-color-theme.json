	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#5c6773"
			}
		},
		{
			"name": "String",
			"scope": [
				"string",
				"constant.other.symbol"
			],
			"settings": {
				"foreground": "#bae67e"
			}
		},
		{
			"name": "Regular Expressions and Escape Characters",
			"scope": [
