			"settings": {
				"foreground": "#95e6cb"
			}
		},
		{
			"name": "Number",
			"scope": [
				"constant.numeric"
			],
			"settings": {
				"foreground": "#ffcc66"
			}
		},
		{
			"name": "Built-in constants",
